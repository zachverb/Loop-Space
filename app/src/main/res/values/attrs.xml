<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoopButton">
        <attr name="cb_color_loop" format="color" />
        <attr name="cb_pressedRingWidth_loop" format="dimension" />
    </declare-styleable>
    <declare-styleable name="LoopProgressBar">
        <attr name="progress_min" format="integer" />
        <attr name="progress_max" format="integer" />
        <attr name="current_progress" format="integer" />
        <attr name="progressbarColor" format="color" />
        <attr name="progressBarThickness" format="dimension" />
    </declare-styleable>
    <color name="primary">#2196F3</color>
    <color name="primary_dark">#1976D2</color>
    <color name="primary_light">#BBDEFB</color>
    <color name="accent">#00BCD4</color>
    <color name="primary_text">#212121</color>
    <color name="secondary_text">#727272</color>
    <color name="icons">#FFFFFF</color>
    <color name="divider">#69d8d8d8</color>
</resources>